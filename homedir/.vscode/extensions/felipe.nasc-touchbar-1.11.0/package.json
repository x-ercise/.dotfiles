				{
					"command": "editor.action.moveSelectionToNextFindMatch",
					"group": "nasc",
					"when": "config.nasc-touchbar.goToNext && !enabledGroup"
				},
				{
					"command": "editor.action.moveSelectionToNextFindMatch",
					"group": "nasc",
					"when": "enabledCursorsGroup"
