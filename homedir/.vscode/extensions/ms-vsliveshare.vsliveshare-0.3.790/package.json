			"view/title": [
				{
					"command": "liveshare.startFromFileTreeExplorer",
					"when": "liveshare:showExplorer && view == vsliveshareTreeExplorer && liveshare:state != Shared && liveshare:state != SharingInProgress && liveshare:state != JoiningInProgress && liveshare:state != Joined",
